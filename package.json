{
    "name": "jumpout",
    "displayName": "jumpout",
    "description": "jumpout",
    "version": "0.0.2",
    "publisher": "twisger",
    "engines": {
        "vscode": "^1.21.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.jumpout",
                "title": "jumpout"
            }
